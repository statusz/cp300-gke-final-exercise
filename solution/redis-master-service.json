{
  "kind": "Service",
  "apiVersion": "v1beta3",
  "metadata": {
    "name": "redis-master",
    "labels": {
      "name": "redis",
      "role": "master"
    }
  },
  "spec": {
    "ports": [
      {
        "protocol": "TCP",
        "port": 6379,
        "targetPort": 6379
      }
    ],
    "selector": {
      "name": "redis",
      "role": "master"
    }
  }
}
